@model List<KurthProject2Vet.Models.PetServices>
@addTagHelper*,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
    ViewBag.Title = "PetServices";
}


<h3>Pet Owners </h3>
<div class="container">
    <div class="row">
        @foreach (var petServices in @Model)
        {
            <div class="col-sm-3">@petServices.FirstName, @petServices.LastName from Store: @petServices.Store.Name </div>
        }
    </div>
</div>
