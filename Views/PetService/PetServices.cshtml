@model List<KurthProject2Vet.Models.PetService>
@addTagHelper*,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
    ViewBag.Title = "PetServices";
}


<h3>Pet Owners </h3>
<div class="container">
    <div class="row">
        @foreach (var petService in @Model)
        {
            <div class="col-sm-3">@petService.Service @petService.DateRendered </div>
        }
    </div>
</div>
